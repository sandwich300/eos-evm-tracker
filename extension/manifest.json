{
  "manifest_version": 3,
  "name": "EOS EVM Asset Tracker",
  "description": "EOS EVM Asset Tracker",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "src/assets/logo.png",
      "48": "src/assets/logo.png",
      "128": "src/assets/logo.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "src/assets/logo.png",
    "48": "src/assets/logo.png",
    "128": "src/assets/logo.png"
  }
}
